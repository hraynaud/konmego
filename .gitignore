# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep
# Ignore uploaded files in development
/storage/*
!/storage/.keep

.byebug_history

# Ignore master key for decrypting credentials and more.
/config/master.key
.env
tags

.DS_Store
node_modules
/dist


!/db/neo4j
/db/neo4j/**
#include subdirectories above conf but ignore all files
!/db/neo4j/test
/db/neo4j/test/**
!/db/neo4j/test/conf
/db/neo4j/test/conf/**
#include neo4j.conf 
!/db/neo4j/test/conf/neo4j.conf


#include subdirectories above conf but ignore all files
!/db/neo4j/development
/db/neo4j/development/**
!/db/neo4j/development/conf
/db/neo4j/development/conf/**
#include neo4j.conf 
!/db/neo4j/development/conf/neo4j.conf

/.bundle
/vendor/bundle
